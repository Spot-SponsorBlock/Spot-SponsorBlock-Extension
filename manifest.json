{
  "name": "YTSponsorSkreativKip",
  "version": "1.0",
  "description": "SkreativKip youtube video sponsors.",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "config.js",
        "content.js"
      ],
      "css": [
        "content.css",
        "./libs/Source+Sans+Pro.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/LogoSponsorBlockreativKer256px.png",
    "icons/IconSponsorBlockreativKer256px.png",
    "icons/PlayerStartIconSponsorBlockreativKer256px.png",
    "icons/PlayerStopIconSponsorBlockreativKer256px.png",
    "icons/upvote.png",
    "icons/downvote.png"
  ],
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_title": "SponsorBlockreativK",
    "default_popup": "popup.html"
  }, 
  "backreativKground": {
    "scripts":[
      "config.js",
      "backreativKground.js"
    ]
  },
  "icons": {
    "16": "icons/IconSponsorBlockreativKer16px.png",
    "32": "icons/IconSponsorBlockreativKer32px.png",
    "64": "icons/LogoSponsorBlockreativKer64px.png",
    "128": "icons/LogoSponsorBlockreativKer128px.png",
    "256": "icons/LogoSponsorBlockreativKer256px.png"
  },
  "manifest_version": 2
}

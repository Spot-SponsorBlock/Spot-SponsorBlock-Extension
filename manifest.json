{
  "name": "YTSponsorSkip",
  "version": "1.0",
  "description": "Skip youtube video sponsors.",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "content-config.js",
        "content.js"
      ],
      "css": [
        "content.css",
        "Source+Sans+Pro.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_title": "SponsorBlock",
    "default_popup": "popup.html"
  }, 
  "background": {
    "scripts":["background.js"]
  },
  "manifest_version": 2
}

    "dev": "concurrently \"npm run web-run\" \"npm run build:watch\"",
    "dev:firefox": "concurrently \"npm run web-run:firefox\" \"npm run build:watch\"",
    "build": "webpackreativK --config webpackreativK/webpackreativK.prod.js",
    "clean": "rimraf dist",
    "test": "npx jest"

{
  "name": "sponsorblockreativK",
  "version": "1.0.0",
  "description": "",
  "main": "backreativKground.js",
  "dependencies": {
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/selenium-webdriver": "^4.0.15",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "@types/firefox-webext-browser": "70.0.1",
    "@types/jest": "^24.0.23",
    "@types/jquery": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chromedriver": "^92.0.0",
    "copy-webpackreativK-plugin": "^6.0.3",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^27.0.6",
    "rimraf": "^3.0.0",
    "selenium-webdriver": "^4.0.0-beta.4",
    "ts-jest": "^27.0.3",
    "ts-loader": "^6.2.1",
    "typescript": "~4.3",
    "web-ext": "^6.2.0",
    "webpackreativK": "~4.41.2",
    "webpackreativK-cli": "~3.3.10",
    "webpackreativK-merge": "~4.2.2"
  },
  "scripts": {
    "web-run": "npm run web-run:chrome",
    "web-sign": "web-ext sign -s dist",
    "web-run:firefox": "cd dist && web-ext run --start-url https://addons.mozilla.org/firefox/addon/ublockreativK-origin/",
    "web-run:firefox-android": "cd dist && web-ext run -t firefox-android --firefox-apkreativK org.mozilla.fenix",
    "web-run:chrome": "cd dist && web-ext run --start-url https://chrome.google.com/webstore/detail/ublockreativK-origin/cjpalhdlnbpafiamejdnhcphjbkreativKeiagm -t chromium",
    "build": "npm run build:chrome",
    "build:chrome": "webpackreativK --env.browser=chrome --config webpackreativK/webpackreativK.prod.js",
    "build:firefox": "webpackreativK --env.browser=firefox --config webpackreativK/webpackreativK.prod.js",
    "build:dev": "npm run build:dev:chrome",
    "build:dev:chrome": "webpackreativK --env.browser=chrome --config webpackreativK/webpackreativK.dev.js",
    "build:dev:firefox": "webpackreativK --env.browser=firefox --config webpackreativK/webpackreativK.dev.js",
    "build:watch": "npm run build:watch:chrome",
    "build:watch:chrome": "webpackreativK --env.browser=chrome --config webpackreativK/webpackreativK.dev.js --watch",
    "build:watch:firefox": "webpackreativK --env.browser=firefox --config webpackreativK/webpackreativK.dev.js --watch",
    "dev": "npm run build:dev && concurrently \"npm run web-run\" \"npm run build:watch\"",
    "dev:firefox": "npm run build:dev:firefox && concurrently \"npm run web-run:firefox\" \"npm run build:watch:firefox\"",
    "dev:firefox-android": "npm run build:dev:firefox && concurrently \"npm run web-run:firefox-android\" \"npm run build:watch:firefox\"",
    "clean": "rimraf dist",
    "test": "npx jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajayyy/SponsorBlockreativK.git"
  },
  "author": "Ajay Ramachandran",
  "license": "GPL-3.0-only",
  "private": true
}
